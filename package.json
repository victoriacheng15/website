{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "format:check": "prettier --check src/ blog/ docs/",
    "format:fix": "prettier --write src/ blog/ docs/",
    "eslint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "lint:check": "pnpm eslint:check && pnpm format:check",
    "lint:fix": "pnpm eslint:fix && pnpm format:fix"
  },
  "dependencies": {
    "@docusaurus/core": "2.2.0",
    "@docusaurus/plugin-ideal-image": "^2.2.0",
    "@docusaurus/preset-classic": "2.2.0",
    "@docusaurus/theme-mermaid": "^2.2.0",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.2.1",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.2.0",
    "@tsconfig/docusaurus": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "prettier": {
    "semi": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 80
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "pnpm": {
    "overrides": {
      "mermaid": "9.1.7"
    }
  }
}
